<configure
   xmlns="http://namespaces.zope.org/zope"
   xmlns:zojax="http://namespaces.zope.org/zojax"
   xmlns:browser="http://namespaces.zope.org/browser"
   i18n_domain="zojax.widget.googlemap">

  <include package="zojax.autoinclude" file="meta.zcml" />
  <includeAllDependencies package="zojax.widget.googlemap" extras="test" />

  <includeOverrides package="zojax.widget.googlemap.tests" file="overrides.zcml" />

  <interface
     name="DefaultSkin"
     interface="zojax.widget.googlemap.tests.tests.IDefaultSkin"
     type="zope.publisher.interfaces.browser.IBrowserSkinType" />

  <browser:defaultSkin name="DefaultSkin" />

  <zojax:content
     title="Page1"
     name="content.page1"
     class="zojax.widget.googlemap.tests.content.Page1"
     schema="zojax.widget.googlemap.tests.content.IPage1"
     permission="zope.Public" />

  <class class="zojax.widget.googlemap.tests.content.Page1">
    <require
       permission="zojax.ModifyContent"
       set_schema="zojax.widget.googlemap.tests.content.IPage1"
       interface="zojax.widget.googlemap.tests.content.IPage1" />
  </class>

  <zojax:content
     title="Page2"
     name="content.page2"
     class="zojax.widget.googlemap.tests.content.Page2"
     schema="zojax.widget.googlemap.tests.content.IPage2"
     permission="zope.Public" />

  <class class="zojax.widget.googlemap.tests.content.Page2">
    <require
       permission="zojax.ModifyContent"
       set_schema="zojax.widget.googlemap.tests.content.IPage2"
       interface="zojax.widget.googlemap.tests.content.IPage2" />
  </class>

  <zojax:content
     title="Page3"
     name="content.page3"
     class="zojax.widget.googlemap.tests.content.Page3"
     schema="zojax.widget.googlemap.tests.content.IPage3"
     permission="zope.Public" />

  <class class="zojax.widget.googlemap.tests.content.Page3">
    <require
       permission="zojax.ModifyContent"
       set_schema="zojax.widget.googlemap.tests.content.IPage3"
       interface="zojax.widget.googlemap.tests.content.IPage3" />
  </class>

  <securityPolicy
      component="zojax.security.securitypolicy.SecurityPolicy" />

  <role id="zope.Anonymous" title="Everybody"
        description="All users have this role implicitly" />
  <role id="zope.Manager" title="Site Manager" />

  <!-- Replace the following directive if you don't want public access -->
  <grant permission="zope.View"
         role="zope.Anonymous" />

  <grant permission="zope.app.dublincore.view"
         role="zope.Anonymous" />

  <!-- Principals -->
  <unauthenticatedPrincipal
      id="zope.anybody"
      title="Unauthenticated User" />

  <!-- Principal that tests generally run as -->
  <principal
      id="zope.mgr"
      title="Manager"
      login="mgr"
      password="mgrpw" />

  <principal
      id="zope.user"
      title="User"
      login="user"
      password="userpw" />

  <everybodyGroup
      id="zope.Everybody"
      title="All Users"
      />

  <grant role="zope.Manager" principal="zope.mgr" />
  <grant role="zope.Manager" principal="zope.user" />

  <grantAll role="zope.Manager" />

</configure>
